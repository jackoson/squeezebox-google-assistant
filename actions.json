                    ]
                }
            },
            "fulfillment": {
                "staticFulfillment": {
                    "templatedResponse": {
                        "items": [
                            {
                                "simpleResponse": {
                                    "textToSpeech": "turning $appliance $state"
                                }
                            },
                            {
                                "deviceExecution": {
                                    "command": "com.example.commands.OnOffAppliance",
                                    "params": {
                                        "appliance": "$appliance",
                                        "state": "$state"
                                    }
                                }
                            }
                        ]
                    }
                }
            }
